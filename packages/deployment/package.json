    "aws-cdk": "2.34.0",
    "esbuild": "0",
    "jest": "^27.5.1",
    "next": "^12.3.0",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
